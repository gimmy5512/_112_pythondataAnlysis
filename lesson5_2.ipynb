   "source": [
    "import random\n",
    "loto=set()\n",
    "\n",
    "while(len(loto)<7):\n",
    "    loto.add(random.randint(1,49))\n",
    "\n",
    "sort_loto=sorted(loto)\n",
    "for item in sort_loto:\n",
    "    print(item,end=\" \")\n",
    "    "
   ]
